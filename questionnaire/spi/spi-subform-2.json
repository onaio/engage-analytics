      }
    },
    {
      "url": "http://hl7.org/fhir/StructureDefinition/variable",
      "valueExpression": {
        "name": "patient-age-space",
        "language": "text/fhirpath",
        "expression": "iif(%patient-age.exists(), ', ' + %patient-age, '')"
      }
    },
    {
      "url": "http://hl7.org/fhir/StructureDefinition/variable",
